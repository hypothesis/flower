{
  "template": "https://github.com/hypothesis/cookiecutters",
  "checkout": null,
  "directory": "pyapp",
  "ignore": [
    ".github/workflows/ci.yml",
    ".github/workflows/keepalive.yml",
    "pyproject.toml",
    "requirements/checkformatting.*",
    "requirements/coverage.*",
    "requirements/format.*",
    "requirements/functests.*",
    "requirements/lint.*",
    "requirements/tests.*",
    "requirements/typecheck.*",
    "Makefile",
    "tox.ini",
    "flower/*",
    "tests/*"
  ],
  "extra_context": {
    "name": "Flower",
    "package_name": "flower",
    "slug": "flower",
    "short_description": "A [Flower](https://flower.readthedocs.io/) instance for Hypothesis, for monitoring and managing our Celery workers and tasks.",
    "python_version": "3.12.8",
    "github_owner": "hypothesis",
    "visibility": "public",
    "copyright_holder": "Hypothesis",
    "dependabot_pip_interval": "monthly",
    "dependabot_npm_interval": "monthly",
    "dependabot_docker_interval": "monthly",
    "devdata": "yes",
    "postgres": "no",
    "docker": "yes",
    "linter": "ruff",
    "__postgres_version": "15.3-alpine",
    "__postgres_port": "34279",
    "__docker_namespace": "hypothesis",
    "__docker_network": "flower_default",
    "__github_url": "https://github.com/hypothesis/flower",
    "__copyright_year": "2025",
    "__parallel_unit_tests": false,
    "__format_runner_type": "ubuntu-latest",
    "__lint_runner_type": "ubuntu-latest",
    "__typecheck_runner_type": "ubuntu-latest",
    "__tests_runner_type": "ubuntu-latest",
    "__coverage_runner_type": "ubuntu-latest",
    "__functests_runner_type": "ubuntu-latest",
    "__scheduled_workflows": "",
    "__slack_notifications_workflows": "",
    "__docker_hub_application_name": "",
    "_checkout": null
  }
}
